<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="com.saasdb.data.GeSgGruXModulos" table="ge_sg_gru_x_modulos" schema="public" dynamic-insert="false" dynamic-update="false">
        <id name="gesggxmid" type="integer">
            <column name="gesggxmid"/>
            <generator class="identity"/>
        </id>
        <property name="gesggxmidmod" type="integer">
            <column name="gesggxmidmod" not-null="true"/>
        </property>
        <many-to-one name="geSgGrupos" class="com.saasdb.data.GeSgGrupos">
            <column name="gesggxmidgrp" not-null="true"/>
        </many-to-one>
    </class>
</hibernate-mapping>