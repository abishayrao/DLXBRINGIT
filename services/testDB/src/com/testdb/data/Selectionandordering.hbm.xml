<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="com.testdb.data.Selectionandordering" table="selectionandordering" dynamic-insert="false" dynamic-update="false">
        <id name="id" type="integer">
            <column name="id"/>
            <generator class="assigned"/>
        </id>
        <property name="preview" type="string">
            <column name="preview"/>
        </property>
        <property name="filter" type="string">
            <column name="filter"/>
        </property>
    </class>
</hibernate-mapping>