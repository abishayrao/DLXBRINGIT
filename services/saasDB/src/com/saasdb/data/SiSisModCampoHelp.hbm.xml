<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="com.saasdb.data.SiSisModCampoHelp" table="si_sis_mod_campo_help" schema="public" dynamic-insert="false" dynamic-update="false">
        <id name="simcahid" type="integer">
            <column name="simcahid"/>
            <generator class="identity"/>
        </id>
        <property name="simcahlingua" type="string">
            <column name="simcahlingua" length="10" not-null="true"/>
        </property>
        <property name="simcahdescricao" type="string">
            <column name="simcahdescricao"/>
        </property>
        <many-to-one name="siSisModCompCampos" class="com.saasdb.data.SiSisModCompCampos">
            <column name="simcahidcampo" not-null="true"/>
        </many-to-one>
    </class>
</hibernate-mapping>